<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:id="@+id/lin1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#C1DBDF"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:id="@+id/lin2"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:background="#C1DBDF"
            android:orientation="horizontal"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/arrow_back23"
                android:layout_width="30dp"
                android:layout_height="70dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="70dp"
                android:layout_toEndOf="@+id/Ali1"
                android:contentDescription="Back Arrow"
                android:src="@drawable/arrow_back" />

            <TextView
                android:id="@+id/textNotifications"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="40dp"
                android:text="Booked Sessions"
                android:textColor="#2196F3"
                android:textSize="27dp"
                android:textStyle="bold" />
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/contactImage1"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/profile" />

            <LinearLayout
                android:layout_toRightOf="@+id/contactImage1"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:layout_width="225dp"
                android:layout_height="60dp"
                android:orientation="horizontal"
                android:background="@color/white"
                >
                <RelativeLayout
                    android:id="@+id/Relative1"
                    android:layout_width="120dp"
                    android:layout_height="100dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/textJohnCooper1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="20dp"
                        android:layout_marginStart="10dp"
                        android:text="John Cooper"
                        android:textColor="#2196F3"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        />
                    <TextView
                        android:id="@+id/textJohnCooper2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="42dp"
                        android:layout_marginStart="10dp"
                        android:text="UX Designer @ Google"
                        android:textColor="@color/black"
                        android:textSize="10dp"
                        android:textStyle="bold"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/Relative2"
                    android:layout_width="120dp"
                    android:layout_height="100dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textdate1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="20dp"
                        android:layout_marginStart="10dp"
                        android:text="24th Dec 2023"
                        android:textColor="#A8970A"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        />
                    <TextView
                        android:id="@+id/texttime1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textdate1"
                        android:layout_gravity="center"
                        android:layout_marginTop="0dp"
                        android:layout_marginStart="10dp"
                        android:text="1:00 pm"
                        android:textColor="#A8970A"
                        android:textSize="13dp"
                        android:textStyle="bold"
                        />


                </RelativeLayout>

            </LinearLayout>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal"
            >

            <ImageView
                android:id="@+id/contactImage2"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                app:tint="@android:color/white"
                android:src="@drawable/profile" />

            <LinearLayout
                android:layout_toRightOf="@+id/contactImage2"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:layout_width="225dp"
                android:layout_height="60dp"
                android:orientation="horizontal"
                android:background="@color/white"
                >
                <RelativeLayout
                    android:id="@+id/Relative3"
                    android:layout_width="120dp"
                    android:layout_height="100dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/textJohnCooper3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="20dp"
                        android:layout_marginStart="10dp"
                        android:text="Emma Phillips"
                        android:textColor="#2196F3"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        />
                    <TextView
                        android:id="@+id/textProfession3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="42dp"
                        android:layout_marginStart="10dp"
                        android:text="Android Developer"
                        android:textColor="@color/black"
                        android:textSize="10dp"
                        android:textStyle="bold"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/Relative4"
                    android:layout_width="120dp"
                    android:layout_height="100dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textdate2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="20dp"
                        android:layout_marginStart="10dp"
                        android:text="1st Jan 2024"
                        android:textColor="#A8970A"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        />
                    <TextView
                        android:id="@+id/texttime2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textdate2"
                        android:layout_gravity="center"
                        android:layout_marginTop="0dp"
                        android:layout_marginStart="10dp"
                        android:text="09:00 pm"
                        android:textColor="#A8970A"
                        android:textSize="13dp"
                        android:textStyle="bold"
                        />
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
