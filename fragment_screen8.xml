<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="com.hamzakhalid.i210704.Screen8"
    android:id="@+id/frame_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:id="@+id/lin1"
        android:layout_width="match_parent"
        android:layout_height="650dp"
        android:background="#C1DBDF"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/arrow_back8"
            android:layout_width="30dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="40dp"
            android:contentDescription="Back Arrow"
            android:src="@drawable/arrow_back" />

        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="30dp"
        android:text="Letâ€™s Find..."
        android:textStyle="bold"
        android:textColor="#2196F3"
        android:textSize="30dp" />

        <androidx.appcompat.widget.SearchView
        android:id="@+id/SearchResults"
        android:layout_marginLeft="40dp"
        android:layout_width="300dp"
        android:queryHint="Search"
        android:layout_height="wrap_content"
        android:background="@color/white"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:text="Recent searches"
        android:textStyle="bold"
        android:textColor="#2196F3"
        android:textSize="20dp" />

    <RelativeLayout
        android:layout_marginStart="10dp"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:orientation="horizontal"
        >
        <RelativeLayout
            android:id="@+id/Rel1"
            android:layout_marginStart="0dp"
            android:layout_width="300dp"
            android:layout_height="200dp"
            android:orientation="vertical"
            >

            <ListView
                android:id="@+id/list"
                android:layout_width="300dp"
                android:textColor="@color/black"
                android:backgroundTint="@color/black"
                android:layout_height="200dp"
                android:layout_marginTop="00dp" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_toRightOf="@+id/Rel1"
            android:layout_marginStart="0dp"
            android:layout_width="60dp"
            android:layout_height="200dp"
            android:orientation="vertical"
            >
            <ImageView
                android:id="@+id/iconClose"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginTop="15dp"
                android:layout_marginStart="10dp"
                android:clickable="true"
                android:contentDescription="Close Icon"
                android:src="@drawable/close" />
            <ImageView
                android:id="@+id/iconClose2"
                android:layout_below="@+id/iconClose"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginTop="35dp"
                android:layout_marginStart="10dp"
                android:clickable="true"
                android:contentDescription="Close Icon"
                android:src="@drawable/close" />
            <ImageView
                android:id="@+id/iconClose3"
                android:layout_below="@+id/iconClose2"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginTop="35dp"
                android:layout_marginStart="10dp"
                android:clickable="true"
                android:contentDescription="Close Icon"
                android:src="@drawable/close" />
        </RelativeLayout>
    </RelativeLayout>
    <LinearLayout
        android:layout_width="360dp"
        android:layout_height="250dp"
        android:orientation="vertical"
        android:background="#B9E9F1"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="30dp"
            android:text="Browse Categories"
            android:textStyle="bold"
            android:textColor="#2196F3"
            android:textSize="25dp" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/contactImage1"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:src="@drawable/profile"
                app:tint="@color/black" />
            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/contactImage1"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="0dp"
                android:text="Entrepreneurship"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="22dp" />

            <ImageView
                android:id="@+id/arrow1"
                android:layout_toRightOf="@id/text1"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginTop="20dp"
                android:layout_marginStart="80dp"
                android:src="@drawable/north_east"
                app:tint="@color/black" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/contactImage2"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                app:tint="@android:color/white"
                android:layout_marginStart="20dp"
                android:src="@drawable/profile"/>
            <TextView
                android:id="@+id/text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/contactImage2"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="0dp"
                android:text="Personal Growth"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="22dp" />

            <ImageView
                android:id="@+id/arrow2"
                android:layout_toRightOf="@id/text2"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginTop="20dp"
                android:layout_marginStart="90dp"
                android:src="@drawable/north_east"
                app:tint="@color/black" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/contactImage3"
                android:layout_width="50dp"
                android:layout_height="50dp"
                app:tint="#BF1919"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:src="@drawable/profile"/>
            <TextView
                android:id="@+id/text3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/contactImage3"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="0dp"
                android:text="Education"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="22dp" />

            <ImageView
                android:id="@+id/arrow3"
                android:layout_toRightOf="@id/text3"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginTop="20dp"
                android:layout_marginStart="150dp"
                android:src="@drawable/north_east"
                app:tint="@color/black" />
        </RelativeLayout>
    </LinearLayout>
    </LinearLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="650dp"
        app:itemTextAppearanceInactive="@style/BottomNavigationView.InactiveText"
        app:labelVisibilityMode="labeled"
        app:menu="@menu/bottom_navigation_menu"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="650dp" />
</FrameLayout>